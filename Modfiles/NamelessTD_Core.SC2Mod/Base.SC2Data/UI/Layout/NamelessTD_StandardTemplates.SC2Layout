                <Anchor side="Bottom" relative="$parent" pos="Max" offset="-5"/>
                <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
                <Texture val="Assets/Textures/ui_nameless_lvsel_pointerarrow.dds"/>
				<Width val="40"/>
				<Height val="24"/>

                <Animation name="Bounce">
                    <Event event="OnShown" action="Play" frame="$this"/>

                    <Controller name="Anchor_0" type="Anchor" end="Loop" side="Bottom" relative="false" unclip="false" frame="$this">
                        <Key type="CurveReal" time="0" inout="Fast,Slow" value="-5"/>
                        <Key type="CurveReal" time="0.8" inout="Slow" value="0"/>
                        <Key type="CurveReal" time="1.6" inout="Fast,Slow" value="-5"/>
                    </Controller>
                </Animation>
            </Frame>

            <Frame type="Button" name="ButtonHitTest">
                <Anchor side="Bottom" relative="$parent" pos="Max" offset="-50"/>
                <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
                <Height val="74"/>
                <Width val="74"/>

                <Frame type="Image" name="NormalImage">
                    <Anchor relative="$parent"/>
                    <Texture val="Assets/Textures/ui_nameless_lvsel_stagebutton_normal.dds"/>
                    <TextureType val="NineSlice"/>
                </Frame>

                <Frame type="Image" name="HoverImage">
                    <Anchor relative="$parent"/>
                    <Texture val="Assets/Textures/ui_nameless_lvsel_stagebutton_over.dds"/>
                    <TextureType val="NineSlice"/>
                </Frame>

                <Animation name="Show">
                    <Event event="OnShown" action="Reset,Play" frame="$this"/>

                    <Controller name="Fade_0" type="Fade" end="Pause" frame="$this">
                        <Key type="CurveReal" time="0" inout="Linear,Slow" value="0"/>
                        <Key type="CurveReal" time="0.2" inout="Slow,Linear" value="255"/>
                    </Controller>
                </Animation>

                <Animation name="ButtonPress">
