    <Frame type="Image" name="QuadrantAnimationTemplate">
        <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
        <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
        <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
        <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
        <TextureType val="NineSlice"/>
        <Texture val="Assets/Textures/ui_nameless_transition_quadrant.dds"/>
        <Alpha val="255"/>
		
            <Frame type="Image" name="GridPattern">
                <Anchor relative="$parent"/>
                <Texture val="Assets\Textures\ui_nova_storymode_bggrid.dds"/>
                <Tiled val="true"/>
                <ColorAdjustMode val="Colorize"/>
                <LightAdjustColor val="5,89,123"/>
                <Alpha val="50"/>
            </Frame>

        <Animation name="RenderIn">
            <Event event="Show" action="Reset,Play"/>
                     
            <Controller type="Dimension" dimension="Height" relative="False" percentage="true" unclip="False" end="Pause" frame="$this">
                <Key type="Curve" time="0" value="200" inout="Auto"/>
                <Key type="Curve" time=".2" value="75" inout="Auto"/>
                <Key type="Curve" time=".4" value="100" inout="Auto"/>
            </Controller>

            <Controller type="Dimension" dimension="Width" relative="False" percentage="true" unclip="False" end="Pause" frame="$this">
                <Key type="Curve" time="0" value="200" inout="Auto"/>
                <Key type="Curve" time=".2" value="75" inout="Auto"/>
                <Key type="Curve" time=".4" value="100" inout="Auto"/>
            </Controller>

            <Controller type="Fade" end="Pause">
                <Key type="Curve" time="0" value="0" inout="Auto"/>
                <Key type="Curve" time=".2" value="150" inout="Auto"/>
                <Key type="Curve" time=".4" value="255" inout="Auto"/>
            </Controller>
        </Animation>

        <Animation name="RenderOut">
            <Event event="Collapse" action="Reset,Play"/>
                     
            <Controller type="Dimension" dimension="Height" relative="False" percentage="true" unclip="False" end="Pause" frame="$this">
                <Key type="Curve" time="0" value="100" inout="Auto"/>
                <Key type="Curve" time=".2" value="75" inout="Auto"/>
                <Key type="Curve" time=".4" value="200" inout="Auto"/>
            </Controller>

            <Controller type="Dimension" dimension="Width" relative="False" percentage="true" unclip="False" end="Pause" frame="$this">
                <Key type="Curve" time="0" value="100" inout="Auto"/>
                <Key type="Curve" time=".2" value="75" inout="Auto"/>
                <Key type="Curve" time=".4" value="200" inout="Auto"/>
            </Controller>

            <Controller type="Fade" end="Pause">
                <Key type="Curve" time="0" value="255" inout="Auto"/>
                <Key type="Curve" time=".2" value="150" inout="Auto"/>
                <Key type="Curve" time=".4" value="0" inout="Auto"/>
            </Controller>
        </Animation>

        <Animation name="CollapseHalfwayShow">
            <Event event="HalfShow" action="Reset,Play"/>
                     
            <Controller type="Dimension" dimension="Height" relative="False" percentage="true" unclip="False" end="Pause" frame="$this">
                <Key type="Curve" time="0" value="200"/>
                <Key type="Curve" time=".2" value="80"/>
            </Controller>

            <Controller type="Dimension" dimension="Width" relative="False" percentage="true" unclip="False" end="Pause" frame="$this">
                <Key type="Curve" time="0" value="200"/>
                <Key type="Curve" time=".2" value="80"/>
            </Controller>

            <Controller type="Fade" end="Pause">
                <Key type="Curve" time="0" value="0" out="Fast"/>
                <Key type="Curve" time=".2" value="150" in="Slow" Out="Slow"/>
            </Controller>
        </Animation>

        <Animation name="CollapseHalfwayHide">
            <Event event="HalfHide" action="Reset,Play"/>
                     
            <Controller type="Dimension" dimension="Height" relative="False" percentage="true" unclip="False" end="Pause" frame="$this">
                <Key type="Curve" time="0" value="80"/>
                <Key type="Curve" time=".2" value="200"/>
            </Controller>

            <Controller type="Dimension" dimension="Width" relative="False" percentage="true" unclip="False" end="Pause" frame="$this">
                <Key type="Curve" time="0" value="80"/>
