        </Frame>

        <Frame type="Image" name="Star3">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent/Star1" pos="Max" offset="0"/>
            <Texture val="Assets\Textures\ui_nameless_stagesel_performancestar_small_normal.dds"/>
