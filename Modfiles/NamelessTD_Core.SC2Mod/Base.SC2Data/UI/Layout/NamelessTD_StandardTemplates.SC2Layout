    <Frame type="Frame" name="StageSlot">
        <Height val="140"/>
        <Width val="240"/>

        <Animation name="Show">
            <Event event="OnShown" action="Reset,Play" frame="$this"/>

            <Controller name="Anchor_0" type="Anchor" end="Pause" side="Bottom" relative="true" unclip="false" frame="$this">
                <Key type="CurveReal" time="0" inout="Linear,Fast" value="-50"/>
                <Key type="CurveReal" time="0.8" inout="Slow,Linear" value="0"/>
            </Controller>

            <Controller name="Fade_0" type="Fade" end="Pause" frame="$this">
                <Key type="CurveReal" time="0" inout="Linear,Slow" value="0"/>
                <Key type="CurveReal" time="0.3" inout="Slow,Linear" value="255"/>
            </Controller>
        </Animation>

