        <Height val="35"/>

		<!-- Disabled Remove from code -->
        <!--Frame type="Image" name="Background">
            <Anchor relative="$parent"/>
            <Texture val="Assets/Textures/ui_nameless_headsup_standardpanel.dds"/>
            <TextureType val="NineSlice"/>
        </Frame-->

        <StateGroup name="Performance">
            <DefaultState val="0Star"/>
            
            <State name="0Star">
                <When type="AnimationState" Performance="0Stars"/>
                <Action type="SetProperty" frame="$this/Star1" texture="Assets\Textures\ui_nameless_stagesel_performancestar_small_normal.dds"/>
                <Action type="SetProperty" frame="$this/Star2" texture="Assets\Textures\ui_nameless_stagesel_performancestar_small_normal.dds"/>
                <Action type="SetProperty" frame="$this/Star3" texture="Assets\Textures\ui_nameless_stagesel_performancestar_small_normal.dds"/>
            </State>

            <State name="1Star">
                <When type="AnimationState" Performance="1Stars"/>
                <Action type="SetProperty" frame="$this/Star1" texture="Assets\Textures\ui_nameless_stagesel_performancestar_small_over.dds"/>
                <Action type="SetProperty" frame="$this/Star2" texture="Assets\Textures\ui_nameless_stagesel_performancestar_small_normal.dds"/>
                <Action type="SetProperty" frame="$this/Star3" texture="Assets\Textures\ui_nameless_stagesel_performancestar_small_normal.dds"/>
            </State>

            <State name="2Star">
                <When type="AnimationState" Performance="2Stars"/>
                <Action type="SetProperty" frame="$this/Star1" texture="Assets\Textures\ui_nameless_stagesel_performancestar_small_over.dds"/>
                <Action type="SetProperty" frame="$this/Star2" texture="Assets\Textures\ui_nameless_stagesel_performancestar_small_over.dds"/>
                <Action type="SetProperty" frame="$this/Star3" texture="Assets\Textures\ui_nameless_stagesel_performancestar_small_normal.dds"/>
            </State>

            <State name="3Star">
                <When type="AnimationState" Performance="3Stars"/>
                <Action type="SetProperty" frame="$this/Star1" texture="Assets\Textures\ui_nameless_stagesel_performancestar_small_over.dds"/>
                <Action type="SetProperty" frame="$this/Star2" texture="Assets\Textures\ui_nameless_stagesel_performancestar_small_over.dds"/>
                <Action type="SetProperty" frame="$this/Star3" texture="Assets\Textures\ui_nameless_stagesel_performancestar_small_over.dds"/>
            </State>
        </StateGroup>

        <Frame type="Image" name="Star1">
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
            <Texture val="Assets\Textures\ui_nameless_stagesel_performancestar_small_normal.dds"/>
