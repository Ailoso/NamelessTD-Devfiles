    <Frame type="Frame" name="PerformancePanelSmall">
        <Anchor side="Bottom" relative="$parent/ButtonHitTest" pos="Max" offset="22"/>
        <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
        <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
