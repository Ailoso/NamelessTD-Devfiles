<Desc>
	<Frame type="Frame" name="MilestonePanelTemplate">
		<Height val="55"/>
					
		<Frame type="Image" name="IconBorder">
			<Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
			<Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
			<Anchor side="Left" relative="$parent" pos="Min" offset="5"/>
			<Height val="55"/>
			<Width val="55"/>
			<Texture val="Assets/Textures/ui_nameless_lvsel_milestone_border.dds"/>
		</Frame>
						
		<Frame type="Image" name="Icon">
			<Anchor side="Top" relative="$parent/IconBorder" pos="Mid" offset="0"/>
			<Anchor side="Bottom" relative="$parent/IconBorder" pos="Mid" offset="0"/>
			<Anchor side="Left" relative="$parent/IconBorder" pos="Mid" offset="0"/>
			<Anchor side="Right" relative="$parent/IconBorder" pos="Mid" offset="0"/>
			<Height val="45"/>
			<Width val="45"/>
			<Texture val="Assets\Textures\alarakex3_ingame_heroportrait.dds"/>
			<ImageMaskTexture val="Assets/Textures/ui_nameless_lvsel_milestone_mask.dds"/>
		</Frame>
						
		<Frame type="Label" name="Title">
			<Anchor side="Top" relative="$parent/Icon" pos="Min" offset="0"/>
			<Anchor side="Left" relative="$parent/Icon" pos="Max" offset="10"/>
			<Anchor side="Right" relative="$parent" pos="Max" offset="15"/>
			<Text val="Milestone"/>
			<Style val="NamelessMilestoneTitle"/>
		</Frame>
		
		<Frame type="ProgressBar" name="CompletionBar">
			<Anchor side="Top" relative="$parent/Title" pos="Max" offset="0"/>
			<Anchor side="Left" relative="$parent/Icon" pos="Max" offset="8"/>
			<AcceptsMouse val="False"/>
			<Width val="135"/>
			<Height val="15"/>
			<UseFillContainer val="true"/>
			<MinValue val="0"/>
			<MaxValue val="100"/>
			<Value val="0"/>
			
			<Frame type="Image" name="BorderImage">
				<Anchor relative="$parent"/>
				<Texture val="Assets/Textures/ui_nameless_lvsel_milestone_progressbar_underlay.dds"/>
			</Frame>
			
			<Frame type="Frame" name="FillImageContainer">
				<Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
				<Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
				<Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
				
				<Frame type="Image" name="FillImage">
					<Anchor relative="$parent"/>
					<Texture val="Assets/Textures/ui_nameless_lvsel_milestone_progressbar_full.dds"/>
				</Frame>
			</Frame>
		</Frame>
		
		<Frame type="TextFormatFrame" name="ProgressValueFormatter">
			<Anchor relative="$parent"/>
			<FormatText val="%0% / %1%"/>
			<Replacement index="0" val="{$parent/CompletionBar/@Value}"/>
			<Replacement index="1" val="{$parent/CompletionBar/@MaxValue}"/>
		</Frame>
		
		<Frame type="Label" name="ProgressValue">
			<Anchor side="Top" relative="$parent/CompletionBar" pos="Max" offset="0"/>
			<Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
			<Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
			<Text val="{$parent/ProgressValueFormatter/@Text}"/>
			<Style val="NamelessMilestoneText"/>
		</Frame>
	</Frame>

