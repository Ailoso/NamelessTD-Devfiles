					</Frame>
					
					<Frame type="Image" name="UpgradePointIcon">
						<Anchor side="Top" relative="$parent/UpgradePointBackground" pos="Mid" offset="0"/>
						<Anchor side="Bottom" relative="$parent/UpgradePointBackground" pos="Mid" offset="0"/>
						<Anchor side="Left" relative="$parent/UpgradePointBackground" pos="Min" offset="10"/>
						<Texture val="Assets\Textures\ui_nameless_stagesel_performancestar_small_normal.dds"/>
						<Height val="50"/>
						<Width val="50"/>
					</Frame>
					
					<Frame type="Label" name="UpgradePointLabel">
						<Anchor side="Top" relative="$parent/UpgradePointBackground" pos="Mid" offset="0"/>
						<Anchor side="Bottom" relative="$parent/UpgradePointBackground" pos="Mid" offset="0"/>
						<Anchor side="Left" relative="$parent/UpgradePointIcon" pos="Max" offset="10"/>
						<Text val="0"/>
						<Style val="NamelessHeroSkillTitle"/>
					</Frame>
					
					<Frame type="Image" name="UpgradePointFrillLeft">
						<Anchor side="Top" relative="$parent/UpgradePointContainer" pos="Mid" offset="0"/>
						<Anchor side="Bottom" relative="$parent/UpgradePointContainer" pos="Mid" offset="0"/>
						<Anchor side="Left" relative="$parent/UpgradePointContainer" pos="Min" offset="0"/>
						<Anchor side="Right" relative="$parent/UpgradePointBackground" pos="Min" offset="-15"/>
						<Color val="1394ff"/>
						<Height val="3"/>
						<Alpha val="150"/>
					</Frame>
					
					<Frame type="Image" name="UpgradePointFrillRight">
						<Anchor side="Top" relative="$parent/UpgradePointContainer" pos="Mid" offset="0"/>
						<Anchor side="Bottom" relative="$parent/UpgradePointContainer" pos="Mid" offset="0"/>
						<Anchor side="Left" relative="$parent/UpgradePointBackground" pos="Max" offset="15"/>
						<Anchor side="Right" relative="$parent/UpgradePointContainer" pos="Max" offset="0"/>
						<Color val="1394ff"/>
						<Height val="3"/>
