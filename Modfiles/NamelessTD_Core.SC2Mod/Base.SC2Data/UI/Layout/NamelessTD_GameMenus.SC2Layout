					</Frame>
					
					<!-- Skill Selection -->
					
					<Frame type="TabControl" name="HeroTabs">
						<Anchor relative="$parent"/>

						<TabListItem val="$parent/SkillSlot1"/>
						<TabListItem val="$parent/SkillSlot2"/>
						<TabListItem val="$parent/SkillSlot3"/>
						<TabListItem val="$parent/SkillSlot4"/>
						<TabListItem val="$parent/SkillSlot5"/>
						<SelectedIndex val="0"/>
					</Frame>
					
					<Frame type="Button" name="SkillSlot1" template="NamelessTD_StandardTemplates/SkillButtonTemplate">
						<Anchor side="Top" relative="$parent" pos="Min" offset="25"/>
						<Anchor side="Left" relative="$parent" pos="Mid" offset="-25"/>
					</Frame>
					
					<Frame type="Button" name="SkillSlot2" template="NamelessTD_StandardTemplates/SkillButtonTemplate">
						<Anchor side="Top" relative="$parent/SkillSlot1" pos="Min" offset="0"/>
						<Anchor side="Left" relative="$parent/SkillSlot1" pos="Max" offset="15"/>
					</Frame>
					
					<Frame type="Button" name="SkillSlot3" template="NamelessTD_StandardTemplates/SkillButtonTemplate">
						<Anchor side="Top" relative="$parent/SkillSlot1" pos="Min" offset="0"/>
						<Anchor side="Left" relative="$parent/SkillSlot2" pos="Max" offset="15"/>
					</Frame>
					
					<Frame type="Button" name="SkillSlot4" template="NamelessTD_StandardTemplates/SkillButtonTemplate">
						<Anchor side="Top" relative="$parent/SkillSlot1" pos="Min" offset="0"/>
						<Anchor side="Left" relative="$parent/SkillSlot3" pos="Max" offset="15"/>
					</Frame>
					
					<Frame type="Button" name="SkillSlot5" template="NamelessTD_StandardTemplates/SkillButtonTemplate">
						<Anchor side="Top" relative="$parent/SkillSlot1" pos="Min" offset="0"/>
						<Anchor side="Left" relative="$parent/SkillSlot4" pos="Max" offset="15"/>
					</Frame>
					
					<Frame type="Frame" name="UpgradePointContainer">
						<Anchor side="Left" relative="$parent/SkillSlot1" pos="Min" offset="0"/>
						<Anchor side="Right" relative="$parent/SkillSlot5" pos="Max" offset="0"/>
						<Anchor side="Top" relative="$parent/SkillSlot1" pos="Max" offset="10"/>
