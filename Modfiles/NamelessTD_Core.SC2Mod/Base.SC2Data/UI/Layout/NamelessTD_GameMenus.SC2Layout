						<Texture val="Assets\Textures\ui_nameless_headsup_standardpanel.dds"/>
						<TextureType val="NineSlice"/>
						<Alpha val="200"/>
					</Frame>
					
					<Frame type="Label" name="SkillTitleLabel">
						<Anchor side="Top" relative="$parent/SkillDisplayBorder" pos="Min" offset="15"/>
						<Anchor side="Left" relative="$parent/SkillDisplayBorder" pos="Mid" offset="0"/>
