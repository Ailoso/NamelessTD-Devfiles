						<Anchor side="Bottom" relative="$parent/SkillDisplayBorder" pos="Max" offset="-15"/>
						<Anchor side="Left" relative="$parent/SkillDisplayBorder" pos="Mid" offset="0"/>
						<Anchor side="Right" relative="$parent/SkillDisplayBorder" pos="Mid" offset="0"/>
