					</Frame>
				</Frame>
			</Frame>
		</Frame>
		
        <Frame type="Frame" name="PausePanel">
            <Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
            <Width val="650"/>
            <Height val="350"/>
            <Visible val="False"/>

            <Animation name="Show">
                <Event event="OnShown" action="DirectionForward,Play"/>
				<Event event="Hide" action="DirectionReverse,Play"/>
                 
                <Controller type="Dimension" dimension="Width" relative="False" percentage="True" unclip="False" end="Pause">
                    <Key type="Curve" time="0" value="0"/>
                    <Key type="Curve" time=".3" value="100"/>
