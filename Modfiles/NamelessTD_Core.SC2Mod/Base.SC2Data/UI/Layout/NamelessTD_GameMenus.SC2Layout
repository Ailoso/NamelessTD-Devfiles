						<Anchor side="Bottom" relative="$parent/BackgroundCutscene" pos="Max" offset="0"/>
						<Anchor side="Left" relative="$parent/BackgroundCutscene" pos="Min" offset="0"/>
						<Anchor side="Right" relative="$parent/BackgroundCutscene" pos="Max" offset="0"/>
						<Texture val="Assets\Textures\ui_battlenet_glue_coop_vinette.dds"/>
					</Frame>
					
					<Frame type="Image" name="Border">
						<Anchor relative="$parent"/>
						<Texture val="Assets\Textures\ui_nameless_standardborder.dds"/>
						<TextureType val="NineSlice"/>
					</Frame>
					
