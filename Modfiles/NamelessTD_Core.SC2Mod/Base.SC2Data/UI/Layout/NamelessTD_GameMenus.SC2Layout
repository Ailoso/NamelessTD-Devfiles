					<Frame type="Button" name="EquipButton" template="NamelessTD_StandardTemplates/SquareButtonTemplate">
						<Anchor side="Top" relative="$parent/HeroDisplay" pos="Max" offset="10"/>
						<Anchor side="Left" relative="$parent/HeroDisplay" pos="Mid" offset="0"/>
						<Anchor side="Right" relative="$parent/HeroDisplay" pos="Mid" offset="0"/>
						<Width val="225"/>
						<Height val="75"/>
						<Style val="LevelSelection_ButtonLabel"/>
