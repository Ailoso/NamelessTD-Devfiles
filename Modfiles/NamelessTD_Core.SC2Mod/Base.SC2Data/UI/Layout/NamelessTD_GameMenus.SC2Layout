				</Frame>
				
				<Frame type="Image" name="Divider">
					<Anchor side="Top" relative="$parent" pos="Min" offset="6"/>
					<Anchor side="Bottom" relative="$parent" pos="Max" offset="-5"/>
					<Anchor side="Left" relative="$parent/HeroListContainer" pos="Max" offset="9"/>
					<Color val="19,148,255"/>
					<Width val="2"/>
					<Alpha val="80"/>
				</Frame>
				
				<Frame type="Frame" name="EditContainer">
					<Anchor side="Top" relative="$parent" pos="Min" offset="15"/>
					<Anchor side="Bottom" relative="$parent" pos="Max" offset="-15"/>
					<Anchor side="Left" relative="$parent/Divider" pos="Max" offset="8"/>
					<Anchor side="Right" relative="$parent" pos="Max" offset="-15"/>
					
