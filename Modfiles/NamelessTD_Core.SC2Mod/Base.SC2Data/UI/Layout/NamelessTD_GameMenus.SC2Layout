						<AlphaLayer val="True" layer="0"/>
									 
						<Color val="000000" layer="1"/>
						<Alpha val="150" layer="1"/>
					</Frame>
					
					<Frame type="Image" name="HeroLevelHoneyComb">
						<Anchor side="Top" relative="$parent/HeroLevelBorder" pos="Mid" offset="0"/>
						<Anchor side="Bottom" relative="$parent/HeroLevelBorder" pos="Mid" offset="0"/>
						<Anchor side="Left" relative="$parent/HeroLevelBorder" pos="Mid" offset="0"/>
						<Anchor side="Right" relative="$parent/HeroLevelBorder" pos="Mid" offset="0"/>
						<Texture val="Assets\Textures\ui_battlenet_glue_coop_commanderlevel_honeycomb.dds"/>
					</Frame>
					
					<Frame type="Label" name="HeroLevelValue">
						<Anchor side="Top" relative="$parent/HeroLevelBorder" pos="Mid" offset="0"/>
						<Anchor side="Bottom" relative="$parent/HeroLevelBorder" pos="Mid" offset="0"/>
						<Anchor side="Left" relative="$parent/HeroLevelBorder" pos="Mid" offset="0"/>
						<Anchor side="Right" relative="$parent/HeroLevelBorder" pos="Mid" offset="0"/>
