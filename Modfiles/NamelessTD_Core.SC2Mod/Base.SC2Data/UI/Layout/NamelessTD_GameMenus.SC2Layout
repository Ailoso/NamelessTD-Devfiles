<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>
    <Frame type="Frame" name="AchievementPanel">
		<Anchor relative="$parent"/>
		
		<Frame type="Frame" name="AchievementAnchor1" template="NamelessTD_StandardTemplates/AchievementPanelTemplate">

			<Animation name="Show">
				<Event event="OnShown" action="Play"/>
				
				<Controller type="Anchor" end="Pause" side="Bottom" relative="false" frame="$this">
					<Key type="Curve" time="0" value="200" out="Fast"/>
					<Key type="Curve" time=".2" value="200" out="Fast"/>
					<Key type="Curve" time=".7" value="-350" in="Slow" out="Slow"/>
					<Key type="Curve" time="1" value="-325" in="Slow"/>
				</Controller>
			</Animation>
		
            <Frame type="Label" name="TitleLabel">
            </Frame>
				
            <Frame type="Label" name="TextLabel">
            </Frame>

			<Frame type="Image" name="Icon">
				<Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
				<Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
				<Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
				<Anchor side="Right" relative="$parent" pos="Mid" offset="0"/> 
			</Frame>
		</Frame>
		
		<Frame type="Frame" name="AchievementAnchor2" template="NamelessTD_StandardTemplates/AchievementPanelTemplate">
		
			<Animation name="Show">
				<Event event="OnShown" action="Play"/>
				
				<Controller type="Anchor" end="Pause" side="Bottom" relative="false" frame="$this">
					<Key type="Curve" time="0" value="200" out="Fast"/>
					<Key type="Curve" time=".2" value="200" out="Fast"/>
					<Key type="Curve" time=".7" value="-225" in="Slow" out="Slow"/>
					<Key type="Curve" time="1" value="-200" in="Slow"/>
				</Controller>
			</Animation>
		
            <Frame type="Label" name="TitleLabel">
            </Frame>
				
            <Frame type="Label" name="TextLabel">
            </Frame>

			<Frame type="Image" name="Icon">
				<Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
				<Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
				<Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
				<Anchor side="Right" relative="$parent" pos="Mid" offset="0"/> 
			</Frame>
		</Frame>
		
		<Frame type="Frame" name="AchievementAnchor3" template="NamelessTD_StandardTemplates/AchievementPanelTemplate">

			<Animation name="Show">
				<Event event="OnShown" action="Play"/>
				
				<Controller type="Anchor" end="Pause" side="Bottom" relative="false" frame="$this">
					<Key type="Curve" time="0" value="200" out="Fast"/>
					<Key type="Curve" time=".2" value="200" out="Fast"/>
					<Key type="Curve" time=".7" value="-100" in="Slow" out="Slow"/>
					<Key type="Curve" time="1" value="-75" in="Slow"/>
				</Controller>
			</Animation>
		
            <Frame type="Label" name="TitleLabel">
            </Frame>
				
            <Frame type="Label" name="TextLabel">
            </Frame>

			<Frame type="Image" name="Icon">
				<Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
				<Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
				<Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
				<Anchor side="Right" relative="$parent" pos="Mid" offset="0"/> 
			</Frame>
		</Frame>
		
		<Frame type="Image" name="Glow">
			<Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
			<Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
			<Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
			<Visible val="False"/>
			<Texture val="Assets/Textures/ui_nameless_achievement_exitglow.dds"/>
			<Alpha val="0"/>
			
			<Animation name="ShootAnimation">
				<Event event="OnShown" action="Play"/>
				 
				<Controller type="Fade" end="Pause">
					<Key type="Curve" time="0" value="0" inout="Auto"/>
					<Key type="Curve" time=".1" value="0" inout="Auto"/>
					<Key type="Curve" time=".4" value="255" inout="Auto"/>
					<Key type="Curve" time=".6" value="0" inout="Auto"/>
				</Controller>

                <Controller type="Visibility" end="Pause">
                    <Key type="Bool" time="0" value="True"/>
                    <Key type="Bool" time=".6" value="False"/>
                </Controller>
			</Animation>
		</Frame>
	</Frame>

    <Frame type="Frame" name="AnchorPanel">
        <Anchor relative="$parent"/>
		
        <Frame type="Frame" name="HeroLoadoutPanel">
            <Anchor relative="$parent"/>
			<Visible val="False"/>
			
			<Animation name="Show">
				<Event event="OnShown" action="Reset,Play"/>
				 
				<Controller type="Fade" end="Pause" frame="$this/BackgroundCover">
					<Key type="Curve" time="0" value="0" out="Slow"/>
					<Key type="Curve" time=".2" value="90" in="Slow"/>
				</Controller>
			</Animation>
			
			<!-- Background Cover and Fullscreen Button -->
			
			<Frame type="Image" name="BackgroundCover">
				<Anchor relative="$parent"/>
				<Color val="000000"/>
				<Alpha val="90"/>
			</Frame>
			
			<Frame type="Button" name="FullscreenButton">
				<Anchor relative="$parent"/>
				
				<Animation name="HidePanel">
					<Event event="OnMouseDown" action="Reset,Play"/>
					 
					<Controller type="Fade" end="Pause" frame="$parent/BackgroundCover">
						<Key type="Curve" time="0" value="90" out="Slow"/>
						<Key type="Curve" time=".2" value="0" in="Slow"/>
					</Controller>
					
					<Controller type="Visibility" end="Pause" frame="$parent">
						<Key type="Bool" time="0" value="True"/>
						<Key type="Bool" time=".2" value="False"/>
					</Controller>
				</Animation>
			</Frame>
			
			<!-- Actual Panel -->
		
			<Frame type="Frame" name="LoadoutPanel">
				<Anchor side="Top" relative="$parent" pos="Mid" offset="0"/>
				<Anchor side="Bottom" relative="$parent" pos="Mid" offset="0"/>
				<Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
				<Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
				<Width val="1350"/>
				<Height val="703"/>
				
				<Frame type="Image" name="Background">
					<Anchor relative="$parent"/>
					<Texture val="Assets\Textures\ui_nameless_headsup_standardpanel.dds"/>
					<TextureType val="NineSlice"/>
					<AcceptsMouse val="True"/>
				</Frame>
				
				<Frame type="Frame" name="HeroListContainer">
					<!-- At some point switch to a scrollable frame -->
					<Anchor side="Top" relative="$parent" pos="Min" offset="12"/>
					<Anchor side="Bottom" relative="$parent" pos="Max" offset="-12"/>
					<Anchor side="Left" relative="$parent" pos="Min" offset="12"/>
					<Width val="300"/>
					
					<Frame type="Image" name="Background">
						<Anchor relative="$parent"/>
						<Texture val="Assets/Textures/ui_nameless_headsup_darkpanel.dds"/>
						<TextureType val="NineSlice"/>
						<Alpha val="150"/>
					</Frame>
					
					<Frame type="Button" name="HeroButton1" template="NamelessTD_StandardTemplates/LoadoutHeroButtonTemplate">
						<Anchor side="Top" relative="$parent" pos="Min" offset="8"/>
						<Anchor side="Left" relative="$parent" pos="Min" offset="8"/>
						
						<!-- Requires changing per hero -->
						
						<Frame type="Image" name="Banner">
							<Texture val="Assets/Textures/ui_nameless_heroloadout_banner_taurenmarine.dds" layer="1"/>
						</Frame>
						
						<Frame type="Image" name="HeroIcon">
							<Texture val="Assets\Textures\ui_nameless_heroloadout_heroicon_taurenmarine.dds"/>
						</Frame>
						
						<Frame type="Label" name="HeroTitle">
							<Anchor side="Top" relative="$parent/HeroBorder" pos="Min" offset="10"/>
							<Anchor side="Left" relative="$parent/HeroBorder" pos="Max" offset="-5"/>
							<Style val="NamelessHeroTauren_Title"/>
							<Text val="Tauren"/>
						</Frame>
					</Frame>
					
					<Frame type="Button" name="HeroButton2" template="NamelessTD_StandardTemplates/LoadoutHeroButtonTemplate">
						<Anchor side="Top" relative="$parent/HeroButton1" pos="Max" offset="10"/>
						<Anchor side="Left" relative="$parent" pos="Min" offset="8"/>
					</Frame>
				</Frame>
				
				<Frame type="TabControl" name="HeroTabs">
					<Anchor relative="$parent"/>

					<TabListItem val="$parent/HeroListContainer/HeroButton1"/>
					<TabListItem val="$parent/HeroListContainer/HeroButton2"/>
