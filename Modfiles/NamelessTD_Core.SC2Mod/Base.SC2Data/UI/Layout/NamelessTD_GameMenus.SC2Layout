						<Height val="80"/>
						<UseFillContainer val="true"/>
						<MinValue val="0"/>
						<MaxValue val="100"/>
						<Value val="0"/>
						
						<Frame type="TextFormatFrame" name="XPValueFormatter">
							<Anchor relative="$parent"/>
							<FormatText val="%0% / %1%"/>
							<Replacement index="0" val="{$parent/@Value}"/>
							<Replacement index="1" val="{$parent/@MaxValue}"/>
						</Frame>
						
						<Frame type="Image" name="BorderImage">
							<Anchor relative="$parent"/>
							<Texture val="Assets/Textures/ui_nameless_heroloadout_expbar_border.dds"/>
							<TextureType val="EndCap"/>
						</Frame>
						
						<Frame type="Image" name="ImageAccent">
							<Anchor relative="$parent"/>
							<Texture val="Assets/Textures/ui_nameless_heroloadout_expbar_background.dds"/>
							<TextureType val="EndCap"/>
						</Frame>
						
						<Frame type="Frame" name="FillImageContainer">
							<Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
							<Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
							<Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
							
