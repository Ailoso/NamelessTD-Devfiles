					<Frame type="Image" name="UpgradePointBackground">
						<Anchor side="Top" relative="$parent/UpgradePointContainer" pos="Min" offset="0"/>
						<Anchor side="Bottom" relative="$parent/UpgradePointContainer" pos="Max" offset="0"/>
